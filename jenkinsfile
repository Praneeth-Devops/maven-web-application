pipeline {
    agent any
    tools {
        maven 'maven3.6.1'
   }

    stages{
        stage('checkout'){
            steps{
               git credentialsId: 'praneegit', url: 'https://github.com/Praneeth-Devops/maven-web-application.git'
            }
        }
        stage('build'){
            steps{
               sh "mvn clean package"
            }
             stage('deploy'){
            steps{
               sh "mvn clean package"
            }
        }
    }
}
